# --- Tasks ---

:MLTask a owl:Class ;
    # rdfs:subClassOf somethingFromExistingOntologies ;
    # owl:disjointWith :OutcomeFairnessNotion ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "Machine-Learning Task"@en ;
    # resource:hasSynonym ""@en ;
    skos:definition "A machine learning task is a type of prediction or inference that's based on both the problem or question and the available data."@en ;
.

:SupervisedLearingTask a owl:Class ;
    rdfs:subClassOf :MLTask ;
    owl:disjointWith :UnsupervisedLearningTask ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "Supervised Learning Task"@en ;
    # resource:hasSynonym ""@en ;
    skos:definition "Supervised learning is a category of machine learning that uses labeled datasets to train algorithms to predict outcomes and recognize patterns."@en ;
.

:UnsupervisedLearningTask a owl:Class ;
    rdfs:subClassOf :MLTask ;
    owl:disjointWith :SupervisedLearingTask ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "Unsupervised Learning Task"@en ;
    # resource:hasSynonym ""@en ;
    skos:definition "Unsupervised tasks involve using algorithms to find patterns, structures, and groupings in unlabeled data without any human guidance or predefined outcomes."@en ;
.

:NaturalLanguageProcessingTask a owl:Class ;
    rdfs:subClassOf :MLTask ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "Natural Language Processing Task"@en ;
    # resource:hasSynonym ""@en ;
    skos:definition "Natural language processing includes breaking down human text or speech into smaller parts that computer programs can easily understand and process."@en ;
.

# --- Scientific Papers ---

:ScientificPaper a owl:Class;
    # rdfs:subClassOf somethingFromExistingOntologies ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "Scientific Paper"@en ;
    # resource:hasSynonym ""@en ;
    skos:definition "A scientific paper published in a journal or during some conference proceesings."@en ;
.

:ScientificArtifact a owl:Class;
    # rdfs:subClassOf somethingFromExistingOntologies ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "Scientific Artifact"@en ;
    # resource:hasSynonym ""@en ;
    skos:definition "A scientific artifact is the outcome with which the the paper contributes, such as a fairness notion, metric, and so on."@en ;
.
# --- Applications ---

:ApplicationDomain a owl:Class;
    # rdfs:subClassOf somethingFromExistingOntologies ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "Application Domain"@en ;
    # resource:hasSynonym ""@en ;
    skos:definition "AI systems are tailored to the application domain of the specific use-case."@en ;
.

# --- ObjectProperties ---

:isGuidedBy a owl:ObjectProperty ;
    rdfs:domain :FairnessNotion ;
    rdfs:range :SocialPrinciple ;
    rdfs:label "is guided by"@en ;
    rdfs:comment "Links a fairness notion to its underpinning social principle such as equity or equality."@en ;
.
:Guides a owl:ObjectProperty ;
    rdfs:domain :SocialPrinciple ;
    rdfs:range :FairnessNotion ;
    rdfs:label "guides"@en ;
    rdfs:comment "Links a fairness notion to its underpinning social principle such as equity or equality."@en ;
.

## --- Metrics and Notions ---

:measures a owl:ObjectProperty ;
    # rdfs:subPropertyOf somethingFromExistingOntologies ;
    rdfs:label "measures"@en ;
    rdfs:domain fmo:fairness_metric ;
    rdfs:range fmo:fairness_notion ;
    skos:definition "A fairness metric measures some fairness notion if comparisons made with the given metric are a valid way to determine fairness with this notion."@en ;
.

:isMeasuredBy a owl:ObjectProperty ;
    # rdfs:subPropertyOf somethingFromExistingOntologies ;
    rdfs:label "is measured by"@en ;
    rdfs:domain fmo:fairness_notion ;
    rdfs:range fmo:fairness_metric ;
    skos:definition "A fairness notion is measured by a fairness metric if comparisons made with the given metric are a valid way to determine fairness with this notion."@en ;
.

## --- Notions and Biases ---

:mitigates a owl:ObjectProperty ;
    # rdfs:subPropertyOf somethingFromExistingOntologies ;
    rdfs:label "mitigates"@en ;
    rdfs:domain fmo:fairness_notion ;
    rdfs:range fmo:bias ;
    skos:definition "A fairness notion is required to mitigate a certain bias."@en ;
.

:isMitigatedBy a owl:ObjectProperty ;
    # rdfs:subPropertyOf somethingFromExistingOntologies ;
    rdfs:label "is mitigated by"@en ;
    rdfs:domain fmo:bias ;
    rdfs:range fmo:fairness_notion ;
    skos:definition "A certain bias in the use-case can be mitigated by enforcing specific fairness notions."@en ;
.

:produces a owl:ObjectProperty ;
    rdfs:subPropertyOf resource:SIO_000215 ;
    rdfs:label "is measurement value of"@en ;
    rdfs:domain :MachineLearningEvaluation ;
    rdfs:range fmo:evaluation_metric ;
    skos:definition "A certain model evaluation produces a certain value w.r.t. a specific metric."@en ;
.

## --- Tasks and ScientificArtifacts ---

:isSuitableFor a owl:ObjectProperty ;
    # rdfs:subPropertyOf somethingFromExistingOntologies ;
    rdfs:label "is suitable for"@en ;
    rdfs:domain :ScientificArtifact ;
    rdfs:range :MLTask ;
    skos:definition "A certain fairness notion is suitable for a certain set of tasks."@en ;
.

## --- Application Domains and ScientificArtifacts ---

:hasBeenAppliedTo a owl:ObjectProperty ;
    # rdfs:subPropertyOf somethingFromExistingOntologies ;
    rdfs:label "has been applied to"@en ;
    rdfs:domain :ScientificArtifact ;
    rdfs:range :ApplicationDomain ;
    skos:definition "A certain fairness notion has been applied to certain application domains."@en ;
.

