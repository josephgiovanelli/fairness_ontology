# --- Classes ---

# --- External ---

obo:IAO_0000030 a owl:Class ;
    rdfs:label "information content entity"@en ;
.

resource:SIO_000006 a owl:Class ;
    rdfs:label "process"@en ;
.

:hasAntonym a rdf:Property, owl:AnnotationProperty ;
    rdfs:label "has antonym"@en ;
    rdfs:comment "Relates a concept to its antonym, given as a string or URI."@en .

# --- Bias ---

fmo:bias a owl:Class ;
    rdfs:subClassOf obo:IAO_0000030 , owl:Thing ;
    dc:source "https://www.lexico.com/en/definition/bias"^^xsd:anyURI ;
    rdfs:label "bias"@en ;
    skos:definition "Predisposition in favor of or against one thing, person, or group compared with another, usually in a way considered to be unfair."@en ;
.

## --- Bias Lev. 1 ---

:HistoricalBias a owl:Class ;
    rdfs:subClassOf fmo:bias ;
    dc:source "https://doi.org/10.1145/3457607"^^xsd:anyURI ;
    rdfs:label "historical bias"@en ;
    skos:definition "Historical bias is caused by already existing bias and socio-technical issues in the world, and can seep into from the data generation process even given a perfect sampling and feature selection."@en
.

:RepresentationBias a owl:Class ;
    rdfs:subClassOf fmo:bias ;
    dc:source "https://doi.org/10.1145/3457607"^^xsd:anyURI ;
    rdfs:label "representation bias"@en ;
    skos:definition "Representation bias arises from the use of a skewed or otherwise non-representative sample from a population during data collection process."@en
.

:IntersectionalBias a owl:Class ;
    rdfs:subClassOf fmo:bias ;
    dc:source "https://doi.org/10.1145/3468507.3468511"^^xsd:anyURI ;
    rdfs:label "intersectional bias"@en ;
    skos:definition "Intersectional bias is a type of bias that occurs due to the combination of multiple sensitive factors."@en
.

:AlgorithmicBias a owl:Class ;
    rdfs:subClassOf fmo:bias ;
    dc:source "https://doi.org/10.1145/3457607"^^xsd:anyURI ;
    rdfs:label "algorithmic bias"@en ;
    skos:definition "Algorithmic bias is when the bias is not present in the input data and is added purely by the algorithm."@en
.

:OmmittedVariableBias a owl:Class ;
    rdfs:subClassOf fmo:bias ;
    dc:source "https://doi.org/10.1145/3457607"^^xsd:anyURI ;
    rdfs:label "omitted variable bias"@en ;
    skos:definition "Omitted variable bias occurs when one or more important variables are left out of a model."@en
.

:MaskingBias a owl:Class ;
    rdfs:subClassOf fmo:bias ;
    dc:source "https://doi.org/10.1145/3468507.3468511"^^xsd:anyURI ;
    rdfs:label "masking bias"@en ;
    skos:definition "Masking is a form of intentional discrimination that allows decision makers with prejudicial views to mask their intentions, typically by exploiting how fairness notions are defined."@en
.

:SamplingBias a owl:Class ;
    rdfs:subClassOf fmo:bias ;
    dc:source "https://doi.org/10.1145/3457607"^^xsd:anyURI ;
    rdfs:label "sampling bias"@en ;
    skos:definition "Similarly to representation bias, sampling bias arises due to non-random sampling of subgroups"@en
.

## --- Bias Lev. 2 ---

:LinguisticBias  a owl:Class ;
    rdfs:subClassOf :AlgorithmicBias ;
    rdfs:label "linguistic bias"@en ;
    skos:definition "Linguistic bias is embedded in generative AI outputs, the manifestation of harm through demeaning or offensive language that reinforces discrimination."@en;
.

## --- Bias Individuals ---

:Prejudice a :HistoricalBias ;
    rdfs:label "prejudice"@en ;
    skos:definition "Prejudice is a latent human or cultural construct and represents historical predispositions to discrimination."@en ;
.

:Stereotype a :RepresentationBias ;
    rdfs:label "stereotype"@en ;
    skos:definition "A simplified and generalized representation of a group (e.g., “women are nurses,” “men are engineers”) that operationalizes discrimination in communication, media, or data."@en ;
.

:Disparagement a :LinguisticBias, :RepresentationBias ;
    rdfs:label "disparagement"@en ;
    skos:definition "Disparagement is not merely bias in representation but bias in expression, it is the linguistic realization of stereotype."@en ;
.

# --- Notions ---

fmo:fairness_notion a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty resource:SIO_000095 ;
        owl:someValuesFrom fmo:Fair-ML_Paradigm
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty resource:SIO_000095 ;
        owl:someValuesFrom fmo:worldview
    ] ;
    dc:source "https://doi.org/10.1145/3468507.3468511"^^xsd:anyURI ;
    rdfs:label "fairness notion"@en ;
    resource:hasSynonym "criterion"@en ;
    resource:hasSynonym "fairness"@en ;
    :hasAntonym "discrimination"@en ;
    skos:definition "A formal definition of fairness that can be observed as a property of a predictive model, in relation to that model's treatment of different cohorts."@en ;
.

## --- Notions Lev. 1 ---

:ProceduralFairnessNotion a owl:Class ;
    rdfs:subClassOf fmo:fairness_notion ;
    # owl:disjointWith :OutcomeFairnessNotion ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "procedural fairness notion"@en ;
    resource:hasSynonym "procedural fairness"@en ;
    skos:definition "Procedural fairness is connected to the concept of fairness from administrative law, which is concerned with equality of treatment within the process that makes a decision."@en ;
.

:OutcomeFairnessNotion a owl:Class ;
    rdfs:subClassOf fmo:fairness_notion ;
    owl:disjointWith :ProceduralFairnessNotion ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "outcome fairness notion"@en ;
    resource:hasSynonym "outcome fairness"@en ;
    skos:definition "Outcome fairness is concerned with the fairness of the results of the decision-making process."@en ;
.
### --- Notions Lev. 2 ---

:IndividualFairnessNotion a owl:Class ;
    rdfs:subClassOf :OutcomeFairnessNotion ;
    owl:disjointWith :GroupFairnessNotion ;
    dc:source "https://doi.org/10.1145/2090236.2090255"^^xsd:anyURI ;
    rdfs:label "individual fairness notion"@en ;
    resource:hasSynonym "individual fairness"@en ;
    skos:definition "A family of fairness notions with the common objective to ensure that similar individuals are treated similarly by a model. Some notions in this group may be a relaxed version of &quot;strict&quot; individual fairness (i.e., they may treat similar indivduals differently under certain conditions)."@en ;
.

:GroupFairnessNotion a owl:Class;
    rdfs:subClassOf :OutcomeFairnessNotion ;
    owl:disjointWith :IndividualFairnessNotion ;
    dc:source "https://doi.org/10.1145/3468507.3468511"^^xsd:anyURI ;
    rdfs:label "group fairness notion"@en ;
    resource:hasSynonym "group fairness"@en ;
    skos:definition "A family of fairness notions with the common objective to ensure that groups who differ by their sensitive attributes are treated equally. (Note: Occasionally, \"group fairness\" is used to refer to statistical parity specifically, instead of the broader meaning as defined here)."@en ;
.

:CausalFairnessNotion a owl:Class ;
    rdfs:subClassOf :OutcomeFairnessNotion ;
    owl:disjointWith :ObservationalFairnessNotion ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "causal fairness notion"@en ;
    resource:hasSynonym "causal fairness"@en ;
    skos:definition "A family of fairness notions focusing on the causal relations between variables."@en ;
.

:ObservationalFairnessNotion a owl:Class ;
    rdfs:subClassOf :OutcomeFairnessNotion ;
    owl:disjointWith :CausalFairnessNotion ;
    # dc:source ""^^xsd:anyURI
    rdfs:label "observational fairness notion"@en ;
    resource:hasSynonym "observational fairness"@en ;
    skos:definition "A family of fairness notions focusing on the joint distribution of observable factors."@en ;
.

#### --- Notions Lev. 3 ---
# They all extend GroupFairnessNotion, shouldn't they extend either CasualFainessNotion or ObservationalFainessNotion?

:IndependenceFairnessNotion a owl:Class ;
    rdfs:subClassOf :GroupFairnessNotion ;
    # owl:disjointWith :SeparationFairnessNotion ;
    # owl:disjointWith :SufficiencyFairnessNotion ;
    dc:source "https://doi.org/10.1145/3468507.3468511"^^xsd:anyURI ;
    rdfs:label "independence fairness notion"@en ;
    resource:hasSynonym "independence fairness"@en ;
    skos:definition "The fairness property satisfied when the predicted outcome variable is independent from protected attributes."@en ;
    fmo:probabilistic_definition "Ŷ ⊥ A"@en ;
.

:SeparationFairnessNotion a owl:Class ;
    rdfs:subClassOf :GroupFairnessNotion ;
    # owl:disjointWith :IndependenceFairnessNotion ;
    # owl:disjointWith :SufficiencyFairnessNotion ;
    dc:source "https://doi.org/10.1145/3468507.3468511"^^xsd:anyURI ;
    rdfs:label "separation fairness notion"@en ;
    resource:hasSynonym "separation fairness"@en ;
    skos:definition "The fairness property satisfied when the predicted outcome variable is independent of protected attributes, conditionally on the actual outcome variable."@en ;
    fmo:probabilistic_definition "Ŷ ⊥ A | Y"@en ;
.

:SufficiencyFairnessNotion a owl:Class ;
    rdfs:subClassOf :GroupFairnessNotion ;
    # owl:disjointWith :SeparationFairnessNotion ;
    # owl:disjointWith :IndependenceFairnessNotion ;
    dc:source "https://doi.org/10.1145/3468507.3468511"^^xsd:anyURI ;
    rdfs:label "sufficiency fairness notion"@en ;
    resource:hasSynonym "sufficiency fairness"@en ;
    skos:definition "The fairness property satisfied when a target variable is independent of protected attributes, conditionally on the predicted outcome variable."@en ;
    fmo:probabilistic_definition "Y ⊥ A | Ŷ"@en ;
.

# --- Metrics ---

# :FairnessMetric a owl:Class ;
#     rdfs:subClassOf fmo:evaluation_metric ;
#     dc:source "https://doi.org/10.1145/3468507.3468511"^^xsd:anyURI ;
#     rdfs:label "fairness metric"@en ;
#     skos:definition "A measurement of some fairness notion in a machine-learning model or dataset, with regards to some specific cohort(s)."@en ;
#
.

fmo:fairness_metric a owl:Class ;
    rdfs:subClassOf fmo:evaluation_metric ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty resource:SIO_000215 ;
        owl:someValuesFrom fmo:fairness_notion
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty resource:SIO_000244 ;
        owl:someValuesFrom fmo:statistical_metric
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty resource:SIO_000668 ;
        owl:someValuesFrom fmo:dataset_cohort
    ] ;
    dc:source "https://doi.org/10.1145/3468507.3468511"^^xsd:anyURI ;
    rdfs:label "fairness metric"@en ;
    skos:definition "A measurement of some fairness notion in a machine-learning model or dataset, with regards to some specific cohort(s)."@en ;
    fmo:tag "hide" ;
.



## --- Metrics Standard ML ---

:ParityBasedFairnessMetric a owl:Class ;
    rdfs:subClassOf fmo:fairness_metric ;
    # dc:source ""^^xsd:anyURI ;
    rdfs:label "parity-based fairness metric"@en ;
    skos:definition "A measurement of any fairness notion, focusing on equalising the probability of a favourable outcome among groups."@en ;
.

:ConfusionMatrixBasedFairnessMetric a owl:Class ;
    rdfs:subClassOf fmo:fairness_metric ;
    # dc:source ""^^xsd:anyURI ;
    rdfs:label "confusion matrix-based fairness metric"@en ;
    skos:definition "A measurement of any fairness notion, focusing on the amount of
correct/incorrect classifications inferred by the predictor."@en ;
.

:ScoreBasedFairnessMetric a owl:Class ;
    rdfs:subClassOf fmo:fairness_metric ;
    # dc:source ""^^xsd:anyURI ;
    rdfs:label "score-based fairness metric"@en ;
    skos:definition "A measurement of any fairness notion, directing attention to the value of the classification score."@en ;
.

:CalibrationBasedFairnessMetric a owl:Class ;
    rdfs:subClassOf fmo:fairness_metric ;
    # dc:source ""^^xsd:anyURI ;
    rdfs:label "calibration-based fairness metric"@en ;
    skos:definition "A measurement of any fairness notion, partially connected to the confusion matrix principles, these metrics are usually referred with the term  calibration-based, with the very aim of distinguishing their prevalent attention to the predictive values."@en ;
.

## --- Metrics LLMs ---

### EmbeddingBasedFairnessMetric
:EmbeddingBasedFairnessMetric a owl:Class ;
    rdfs:subClassOf fmo:fairness_metric ;
    rdfs:label "embedding-based fairness metric"@en ;
    skos:definition "A fairness metric that evaluates bias or fairness using representations derived from word or sentence embeddings."@en
.

:WordEmbeddingFairnessMetric a owl:Class ;
    rdfs:subClassOf :EmbeddingBasedFairnessMetric ;
    rdfs:label "word embedding fairness metric"@en ;
    skos:definition "A metric that leverages word vector representations to assess fairness or bias in language models or datasets."@en
.
:SentenceEmbeddingFairnessMetric a owl:Class ;
    rdfs:subClassOf :EmbeddingBasedFairnessMetric ;
    rdfs:label "sentence embedding fairness metric"@en ;
    skos:definition "A metric using sentence-level embedding representations as the basis for evaluating model or data fairness."@en
.

### ProbabilityBasedFairnessMetric
:ProbabilityBasedFairnessMetric a owl:Class ;
    rdfs:subClassOf fmo:fairness_metric ;
    rdfs:label "probability-based fairness metric"@en ;
    skos:definition "A metric that considers the probability distributions of predicted outcomes to measure fairness."@en
.
:MaskedTokenFairnessMetric a owl:Class ;
    rdfs:subClassOf :ProbabilityBasedFairnessMetric ;
    rdfs:label "masked token fairness metric"@en ;
    skos:definition "A fairness metric relying on the predicted probabilities for masked tokens in language models."@en
.
:PseudoLogLikelihoodFairnessMetric a owl:Class ;
    rdfs:subClassOf :ProbabilityBasedFairnessMetric ;
    rdfs:label "pseudo-log likelihood fairness metric"@en ;
    skos:definition "A metric based on pseudo-log likelihoods, often used to evaluate the fairness of generation or prediction probabilities."@en
.

### GeneratedTextBasedFairnessMetric
:GeneratedTextBasedFairnessMetric a owl:Class ;
    rdfs:subClassOf fmo:fairness_metric ;
    rdfs:label "generated text-based fairness metric"@en ;
    skos:definition "A metric that measures fairness by analyzing or scoring generated text outputs."@en
.
:DistributionFairnessMetric a owl:Class ;
    rdfs:subClassOf :GeneratedTextBasedFairnessMetric ;
    rdfs:label "distribution fairness metric"@en ;
    skos:definition "A metric that evaluates fairness by comparing the statistical distribution of generated outputs across groups."@en
.
:ClassifierFairnessMetric a owl:Class ;
    rdfs:subClassOf :GeneratedTextBasedFairnessMetric ;
    rdfs:label "classifier fairness metric"@en ;
    skos:definition "A metric that employs a classifier on generated text to assess the presence of bias or fairness."@en
.
:LexiconFairnessMetric a owl:Class ;
    rdfs:subClassOf :GeneratedTextBasedFairnessMetric ;
    rdfs:label "lexicon fairness metric"@en ;
    skos:definition "A metric that uses predefined lexicons to study fairness or bias in generated textual content."@en
.

## --- Social Principles ---

:SocialPrinciple a owl:Class ;
    rdfs:label "Social Principle"@en ;
    skos:definition "Ethical and contextual interpretation of fairness, informing notions."@en ;
.

### --- Social Principles Individuals ---

:EquityPrinciple a :SocialPrinciple ;
    rdfs:label "equity"@en ;
    skos:definition "Equity is providing different resources based on individual needs to ensure everyone has an equal outcome."@en ;
.

:EquilityPrincipal a :SocialPrinciple ; 
    rdfs:label "equility"@en ;
    skos:definition "Equality is giving everyone the same resources and opportunities."@en ;
.

